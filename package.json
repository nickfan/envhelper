    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "nickfan/envhelper",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT"
}
