{
  "name": "envhelper",
  "version": "0.0.0",
  "description": "Environment Label Helper Class",
  "homepage": "https://github.com/nickfan/envhelper",
  "author": {
    "name": "nick fan",
    "email": "",
    "url": "https://github.com/nickfan"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "env",
    "helper",
    "util",
    "label"
  ],
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "prettier": "^1.15.2",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-config-xo": "^0.25.0",
    "jest": "^23.5.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "lint-staged": {
